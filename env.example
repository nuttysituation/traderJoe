# Application
NODE_ENV=development
PORT=8765
HTTPS_PORT=8766
FRONTEND_URL=https://localhost:3000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=stock_tracker

# Redis
REDIS_URL=redis://localhost:6379

# Free API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key
IEX_CLOUD_API_KEY=your_iex_cloud_key
OPEN_SECRETS_API_KEY=your_open_secrets_key

# Free Alternative APIs
POLYGON_API_KEY=your_polygon_key
FINNHUB_API_KEY=your_finnhub_key
FMP_API_KEY=your_financial_modeling_prep_key

# Paid APIs (Optional - for enhanced data)
CAPITOL_TRADES_API_KEY=your_capitol_trades_key
QUIVER_QUANT_API_KEY=your_quiver_quant_key

# AI Service
AI_SERVICE_URL=http://localhost:8000

# Logging
LOG_LEVEL=info

# Security
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Data Collection
DATA_COLLECTION_INTERVAL=3600000
STOCK_PRICE_UPDATE_INTERVAL=30000
TRADE_DATA_UPDATE_INTERVAL=86400000

# Web Scraping Settings (for free alternatives)
SCRAPING_ENABLED=true
SCRAPING_DELAY=2000
USER_AGENT=Mozilla/5.0 (compatible; StockTracker/1.0) 