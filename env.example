# Application
NODE_ENV=development
PORT=8765
HTTPS_PORT=8766
FRONTEND_URL=https://localhost:3000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_secure_password_here
DB_NAME=stock_tracker

# Redis
REDIS_URL=redis://localhost:6379

# Free API Keys (Get free keys from these services)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
IEX_CLOUD_API_KEY=your_iex_cloud_key_here
OPEN_SECRETS_API_KEY=your_open_secrets_key_here

# Free Alternative APIs
POLYGON_API_KEY=your_polygon_key_here
FINNHUB_API_KEY=your_finnhub_key_here
FMP_API_KEY=your_financial_modeling_prep_key_here

# Paid APIs (Optional - for enhanced data)
CAPITOL_TRADES_API_KEY=your_capitol_trades_key_here
QUIVER_QUANT_API_KEY=your_quiver_quant_key_here

# AI Service
AI_SERVICE_URL=http://localhost:8000

# Logging
LOG_LEVEL=info

# Security (Generate strong random keys)
JWT_SECRET=generate_a_strong_random_jwt_secret_here
ENCRYPTION_KEY=generate_a_strong_random_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Data Collection
DATA_COLLECTION_INTERVAL=3600000
STOCK_PRICE_UPDATE_INTERVAL=30000
TRADE_DATA_UPDATE_INTERVAL=86400000

# Web Scraping Settings (for free alternatives)
SCRAPING_ENABLED=true
SCRAPING_DELAY=2000
USER_AGENT=Mozilla/5.0 (compatible; StockTracker/1.0)

# Security Notes:
# - Never commit this file with real API keys
# - Use strong, unique passwords for database
# - Generate random secrets for JWT and encryption
# - Rotate API keys regularly
# - Use environment-specific configurations 